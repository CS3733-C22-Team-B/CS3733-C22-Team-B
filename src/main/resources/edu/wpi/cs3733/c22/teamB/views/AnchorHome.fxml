                                 <image>
                                    <Image url="@../images/icons/thin-icons/icons8-search-50.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </JFXButton>
                        <JFXButton fx:id="settingsButton" onAction="#goToSettings" text=" ">
                           <graphic>
                              <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
