            <HBox nodeOrientation="LEFT_TO_RIGHT" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="TOP_CENTER" prefWidth="70.0" spacing="17.0" style="-fx-background-color: #ffffff;">
                     <children>
                        <JFXButton fx:id="homeButton" onAction="#goHome" text=" ">
                           <graphic>
                              <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/icons/thin-icons/icons8-home-50.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </JFXButton>
                        <JFXButton fx:id="mapButton" onAction="#goToMap" text=" ">
